{  
  "name": "Fantia downloader",  
  "version": "3.1.9",  
  "description": "Download your Fantia rewards more easily! ",
  "default_locale": "en",
  "manifest_version": 2,
  "author": "suzumiyahifumi",
  "icons": { "64": "icon_64.png" },
  "content_scripts": [
    {  
      "matches": ["https://fantia.jp/posts/*", "https://fantia.jp/fanclubs/*/backnumbers*"],  
      "js": [
        "jquery-3.4.1.js",
        "zip-full.min.js",
        "userscriptAdapter.js",
        "FantiaDownloader.js"
      ]
    } 
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "host_permissions": [
    "*://*.fantia.jp/*"
  ],
  "permissions": [
    "webRequest",
    "downloads",
    "storage"
  ]
}